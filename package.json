{
  "name": "bracelet-studio-v1.10",
  "private": true,
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"{src,public,.}/**/*.{ts,tsx,js,jsx,json,css,html,md}\"",
    "format:check": "prettier --check \"{src,public,.}/**/*.{ts,tsx,js,jsx,json,css,html,md}\"",
    "doctor:eslint": "npm ls eslint eslint-plugin-react-hooks @typescript-eslint/eslint-plugin || true",
    "prepare": "husky",
    "precommit": "lint-staged",
    "check": "npm run format:check && npm run lint",
    "prepack": "npm run format && npm run lint:fix"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.179.1",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.179.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.0"
  },
  "description": "Bracelet Studio v1",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,js,jsx,css,md,json,html}": [
      "prettier --write"
    ]
  }
}
